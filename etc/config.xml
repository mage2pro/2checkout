<?xml version='1.0'?>
<config
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='urn:magento:module:Magento_Store:etc/config.xsd'
>
    <default>
		<df_payment>
			<two_checkout>
				<title>Bank Card</title>
				<!-- 2016-03-15 -->
				<title_backend>2Checkout</title_backend>
			</two_checkout>
		</df_payment>
        <payment>
            <dfe_two_checkout>
                <!--
                 	2016-02-15
                	https://github.com/magento/magento2/blob/6ce74b2/app/code/Magento/Payment/Helper/Data.php#L88-L91
                -->
				<model>Dfe\TwoCheckout\Method</model>
				<!--
				 	2016-05-06
				 	По аналогии с https://mage2.pro/t/topic/898/3
				-->
				<paymentInfoKeys></paymentInfoKeys>
				<privateInfoKeys></privateInfoKeys>
            </dfe_two_checkout>
        </payment>
    </default>
</config>
